@article{guellrousPulmonaryRehabilitationSociedad2014,
  title = {{Pulmonary rehabilitation. Sociedad Espa\~nola de Neumolog\'ia y Cirug\'ia Tor\'acica (SEPAR).}},
  author = {G{\"u}ell Rous, Mar{\'i}a Rosa and D{\'i}az Lobato, Salvador and Rodr{\'i}guez Trigo, Gema and Morante V{\'e}lez, F{\'a}tima and San Miguel, Marta and Cejudo, Pilar and Ortega Ruiz, Francisco and Mu{\~n}oz, Alejandro and Galdiz Iturri, Juan Bautista and Garc{\'i}a, Almudena and Servera, Emilio},
  year = {2014},
  month = aug,
  journal = {Arch Bronconeumol},
  volume = {50},
  number = {8},
  pages = {332--344},
  address = {{Spain}},
  issn = {1579-2129 0300-2896},
  doi = {10.1016/j.arbres.2014.02.014},
  abstract = {Pulmonary rehabilitation (PR) has been shown to improve dyspnea, exercise capacity and health-related quality of life in patients with chronic obstructive pulmonary  disease (COPD). PR has also shown benefits in diseases other than COPD but the level  of evidence is lower. The fundamental components of PR programs are muscle training,  education and chest physiotherapy. Occupational therapy, psychosocial support and  nutritional intervention should also be considered. Home programs have been shown to  be as effective as hospital therapy. The duration of rehabilitation programs should  not be less than 8 weeks or 20 sessions. Early initiation of PR, even during  exacerbations, has proven safe and effective. The use of oxygen or noninvasive  ventilation during training is controversial and dependent on the patient's  situation. At present, the best strategy for maintaining the benefits of PR in the  long term is unknown. Longer PR programs or telemedicine could play a key role in  extending the results obtained.},
  copyright = {Copyright \textcopyright{} 2013 SEPAR. Published by Elsevier Espana. All rights reserved.},
  langid = {spanish},
  pmid = {24845559},
  keywords = {Breathing Exercises,Chronic obstructive pulmonary disease,Combined Modality Therapy,Disease Management,Diseases other than chronic obstructive pulmonary disease,Enfermedad pulmonar obstructiva crónica,Enfermedades distintas de la enfermedad pulmonar obstructiva crónica,Entrenamiento muscular,Exacerbaciones,Exacerbations,Exercise,Home Care Services,Humans,Location and duration of the programs,Lung Diseases/rehabilitation,Lung Neoplasms/rehabilitation/surgery,Maintenance,Mantenimiento,Muscle training,Nutritional Support,Obesity/complications/diet therapy,Occupational Therapy,Oxygen Inhalation Therapy,Patient Care Team,Patient Education as Topic,Physical Therapy Modalities,Positive-Pressure Respiration,Pulmonary Disease; Chronic Obstructive/complications/*rehabilitation,Rehabilitación respiratoria,Relaxation Therapy,Resistance Training,Respiratory therapy,Respiratory Therapy,Social Support,Ubicación y duración de los programas}
}

