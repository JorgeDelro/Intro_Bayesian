@article{shettyEstimationLegPower2002a,
  title = {Estimation of Leg Power: A Two-Variable Model.},
  author = {Shetty, Anand B},
  year = {2002},
  month = jul,
  journal = {Sports biomechanics},
  volume = {1},
  number = {2},
  pages = {147--55},
  doi = {10.1080/14763140208522793},
  abstract = {Leg power is an essential component for success in sports and athletic performance. Therefore, the leg power measurement may help athletes, coaches, athletic trainers, and rehabilitation specialists in selecting, treating, and training athletes for a specific sport. Using a conventional 'jump and reach' test, one can accurately predict the leg power and success in anaerobic-type sports. Nineteen untrained male subjects performed 'jump and reach' vertical jumps on a force platform. Power values were calculated from the force versus time data obtained from the force platform. A regression equation was obtained to predict the power values using the weight of an individual and the 'jump and reach' height as independent variables. The regression equation is given by p = -666.3 + 14.74 [Mass (kg)] + 1925.72 [Height (m)]; [R-square = 0.69, p \textbackslash textless 0.05].},
  keywords = {Force platform,Impulse,Power,Vertical jump}
}

