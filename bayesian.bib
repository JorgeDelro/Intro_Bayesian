  keywords = {Adolescent,Adult,Attitudes,Children,Diabetes Mellitus,Female,Health Knowledge,Humans,Male,Practice/*ethnology,School,School Teachers,Spain,Students,Surveys and Questionnaires,Teachers,Type 1 diabetes,Type 1/*diagnosis/pathology},
