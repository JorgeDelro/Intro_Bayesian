@article{gonzalez-montesinosNewNasalRestriction2020,
  title = {A {{New Nasal Restriction Device Called FeelBreathe}}(\textregistered ) {{Improves Breathing Patterns}} in {{Chronic Obstructive Pulmonary Disease Patients}} during {{Exercise}}.},
  author = {{Gonzalez-Montesinos}, Jos{\'e} L. and Arnedillo, Aurelio and {Fernandez-Santos}, Jorge R. and {Vaz-Pardal}, Carmen and Garc{\'i}a, Pelayo A. and {Castro-Pi{\~n}ero}, Jos{\'e} and {Ponce-Gonz{\'a}lez}, Jes{\'u}s G.},
  year = {2020},
  month = jul,
  journal = {Int J Environ Res Public Health},
  volume = {17},
  number = {13},
  issn = {1660-4601 1661-7827},
  doi = {10.3390/ijerph17134876},
  abstract = {A device called FeelBreathe (FB)(\textregistered ) was designed, developed, and patented for inspiratory muscle training. The main aim was to determine the acute responses on  lung ventilation, gas exchange, and heart rate during exercise in patients with  chronic obstructive pulmonary disease (COPD) with and without the use of FB. In this  study, a randomized cross-over trial was performed with 18 men diagnosed with COPD  (FEV(1) between 30\% and 70\% of its predicted value). Each participant randomly  conducted two trials with 30 min of rest between them with the same protocol on a  treadmill for 10 min at a constant rate of 50\% of VO(2peak). Each test was performed  randomly and in a crossover randomized design in two different conditions: (1)  oronasal breathing; and (2) nasal breathing with FB (nasal ventilatory flow  restriction device). It was observed that FB had positive effects on dynamic  hyperinflation, breathing pattern, and breathing efficiency, with higher expiratory  and inspiratory time. Despite these differences, blood oxygen saturation percentage,  oxygen uptake, and heart rate showed a similar response for both conditions during  exercise. The results suggest that exercise performed with FB improved ventilatory  responses compared to the oronasal mode in COPD patients. This new tool could be  used during most daily tasks and exercise programs.},
  langid = {english},
  pmcid = {PMC7370091},
  pmid = {32640755},
  keywords = {*COPD,*exercise,*Pulmonary Disease; Chronic Obstructive,*respiratory muscle training,Aged,Exercise,Exercise Test,Exercise Tolerance,Humans,Male,Middle Aged,Respiration}
}

