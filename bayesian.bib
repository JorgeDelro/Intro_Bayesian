@article{decastro-maquedaSunProtectionHabits2020,
  title = {Sun {{Protection Habits}} and {{Sunburn}} in {{Elite Aquatics Athletes}}: {{Surfers}}, {{Windsurfers}} and {{Olympic Sailors}}.},
  author = {{De Castro-Maqueda}, Guillermo and {Gutierrez-Manzanedo}, J. V. and {Ponce-Gonz{\'a}lez}, J. G. and {Fernandez-Santos}, J. R. and {Linares-Barrios}, M. and {De Troya-Mart{\'i}n}, M.},
  year = {2020},
  month = apr,
  journal = {J Cancer Educ},
  volume = {35},
  number = {2},
  pages = {312--320},
  address = {{England}},
  issn = {1543-0154 0885-8195},
  doi = {10.1007/s13187-018-1466-x},
  abstract = {The objective of this study was to analyse the sun exposure habits of elite aquatics athletes, together with their sunburn history and skin examination habits, taking  into account differences by sex and between sports. A cross-sectional health survey  was conducted at world championship events during which elite surfers, windsurfers  and Olympic sailors from 30 different countries were invited to participate. The  athletes were asked to complete a self-administered questionnaire focused on sun  protection habits, sun exposure and sports practices. The survey was completed by  240 athletes, 171 men (71.3\%) and 69 women (28.8\%), with a mean age of 22~years (SD  5.86) and skin phototypes I (6.3\%), II (3.3\%), III (22.0\%), IV (32.5\%), V (9.2\%) and  VI (1.7\%). There were significant differences between male and female athletes in  skin self-examination (p~{$<$}\,0.001) and in the use of sunglasses (p~{$<$}\,0.001). In  sunburn history during the last season, there were significant differences  (p~{$<$}\,0.001) among the three sports analysed, in both sexes. The mean rate of sunburn  during the previous season was 76.7\%, and 27.5\% of participants reported having  experienced three or more sunburns that lasted at least 1~day. This type of athlete  is at high risk of sunburn and hence of future skin cancer. However, there is a  general lack of awareness of this risk. This study highlights the need to inform  such athletes and to raise awareness of the dangers, through educational  interventions on the importance of protecting against sunburn in order to reduce the  future incidence of skin cancer.},
  langid = {english},
  pmid = {30637576},
  keywords = {*Aquatics athletes,*Habits,*Health Knowledge; Attitudes; Practice,*Skin cancer,*Sun protection,*Sunburn,*Sunscreen use,Adolescent,Adult,Athletes/education/*psychology,Cross-Sectional Studies,Female,Humans,Incidence,Male,Skin Care/*psychology/statistics \& numerical data,Skin Neoplasms/epidemiology/*prevention \& control/psychology,Spain/epidemiology,Sunburn/epidemiology/*prevention \& control/psychology,Sunscreening Agents/therapeutic use,Surveys and Questionnaires,Young Adult}
}

