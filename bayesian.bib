@article{arnedilloEffectsRehabilitationProgramme2020,
  title = {Effects of a {{Rehabilitation Programme}} with a {{Nasal Inspiratory Restriction Device}} on {{Exercise Capacity}} and {{Quality}} of {{Life}} in {{COPD}}.},
  author = {Arnedillo, Aurelio and {Gonzalez-Montesinos}, Jose L. and {Fernandez-Santos}, Jorge R. and {Vaz-Pardal}, Carmen and {Espa{\~n}a-Dom{\'i}nguez}, Carolina and {Ponce-Gonz{\'a}lez}, Jes{\'u}s G. and {Cuenca-Garc{\'i}a}, Magdalena},
  year = {2020},
  month = may,
  journal = {Int J Environ Res Public Health},
  volume = {17},
  number = {10},
  issn = {1660-4601 1661-7827},
  doi = {10.3390/ijerph17103669},
  abstract = {OBJECTIVE: The objective was to assess the effects of a nasal restriction device for inspiratory muscle training, called Feelbreathe(\textregistered ), added to a rehabilitation  program (RP) on exercise capacity, quality of life, dyspnea and inspiratory muscle  strength in patients with stable COPD. METHODS: Patients were randomized into three  groups, one performed a supervised RP using the Feelbreathe(\textregistered ) device (FB group),  the second group developed the same RP with oronasal breathing without FB (ONB  group) and the third was the control group (CG). We evaluated inspiratory muscle  strength (P(Imax)), dyspnea (mMRC), quality of life (CAT) and exercise capacity  (6MWT) before and after 8-week of RP. RESULTS: A total of 16 patients completed the  study, seven in FB group, five in ONB group and four in the CG. After the RP, the FB  group showed a significant increase in P(Imax) (93.3 {$\pm$} 19.1 vs. 123.0 {$\pm$} 15.8 mmHg)  and in the 6MWT distance (462.9 {$\pm$} 71.8m vs. 529.1 {$\pm$} 50.1 m) and a decrease in the  CAT score (9.7 {$\pm$} 6.5 vs. 5.9 {$\pm$} 6.0) and in the mMRC dyspnea score. FB provides  greater improvement in P(Imax), dyspnea, quality of life and 6MWT than ONB.  CONCLUSIONS: The Feelbreathe(\textregistered ) device provides greater improvements in quality of  life, dyspnea, exercise capacity and inspiratory muscle strength compared to  patients that did not use it.},
  langid = {english},
  pmcid = {PMC7277218},
  pmid = {32456097},
  keywords = {*COPD,*Exercise Tolerance,*inspiratory muscle training,*medical device,*Pulmonary Disease; Chronic Obstructive/therapy,*pulmonary rehabilitation,*Respiratory Muscles,Breathing Exercises/*instrumentation,Dyspnea,Humans,Quality of Life}
}

