@article{gutierrez-manzanedoTeachersKnowledgeType2018,
  title = {Teachers' Knowledge about Type 1 Diabetes in South of {{Spain}} Public Schools.},
  author = {{Guti{\'e}rrez-Manzanedo}, Jos{\'e} V. and {Carral-San Laureano}, Florentino and {Moreno-Vides}, Pablo and {de Castro-Maqueda}, Guillermo and {Fern{\'a}ndez-Santos}, Jorge R. and {Ponce-Gonz{\'a}lez}, Jes{\'u}s Gustavo},
  year = {2018},
  month = sep,
  journal = {Diabetes Res Clin Pract},
  volume = {143},
  pages = {140--145},
  address = {{Ireland}},
  issn = {1872-8227 0168-8227},
  doi = {10.1016/j.diabres.2018.07.013},
  abstract = {AIMS: To evaluate the knowledge of teachers in educative public centres (pre-, primary, and secondary schools) about type 1 diabetes (T1D) in the Puerto Real  University Hospital area (Cadiz, Spain). METHODS: Descriptive observational study in  which 756 teachers' answers on the Test of Diabetes Knowledge for Teachers (TDKT)  were analysed. Teachers from 44 educative public centres in the Puerto Real  University Hospital area (Cadiz, Spain) were selected by randomized sampling (mean  age, 44.3\,{$\pm$}\,8\,years; 61.7\% women). RESULTS: Although 43.2\% survey respondents  recognised having had or currently having children or adolescent students with T1D,  most demonstrated they did not have enough knowledge about T1D (mean score,  6.0\,{$\pm$}\,4.3 points from a maximum of 16), and only 5.1\% had sufficient knowledge  (13-16 points) to be an effective support person to students with diabetes in  school. However, the vast majority of survey respondents correctly answered  questions about symptoms (74.2\%) and hypoglycaemia treatment (85.9\%). Univariate  analysis of factors associated with teachers' knowledge about diabetes revealed that  teachers with {$\leq$}\,15\,years of teaching experience and physical education teachers had  better knowledge than their co-workers. CONCLUSIONS: It is necessary to improve  teachers' knowledge about diabetes in our health area with the aim of improving the  security of children and adolescent students with T1D in school centres.},
  copyright = {Copyright \textcopyright{} 2018 Elsevier B.V. All rights reserved.},
  langid = {english},
  pmid = {30006308},
  keywords = {Adolescent,Adult,Children,Diabetes Mellitus; Type 1/*diagnosis/pathology,Female,Health Knowledge; Attitudes; Practice/*ethnology,Humans,Male,School,School Teachers,Spain,Students,Surveys and Questionnaires,Teachers,Type 1 diabetes}
}

